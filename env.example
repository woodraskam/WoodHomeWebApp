# WoodHome WebApp Environment Configuration
# Copy this file to .env and update the values

# WebApp Configuration
PORT=3000

# WoodHome API Configuration
# For local development
WOODHOME_API_URL=http://localhost:8080

# For Cloudflare tunnel (uncomment when using tunnel)
# WOODHOME_API_URL=https://api.markwoodraska.com

# For production (uncomment when deployed)
# WOODHOME_API_URL=https://your-production-api.com

# Optional: Custom domain for WebApp
# WEBAPP_DOMAIN=markwoodraska.com
